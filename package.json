{
    "name": "solidjs-assessments",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev:vite": "vite",
        "dev:tsc": "tsc --build --watch",
        "dev": "run-p dev:*",
        "build": "tsc --build && vite build",
        "preview": "vite preview",
        "lint:eslint": "eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,ts,tsx,cjs,mjs}\"",
        "lint:stylelint": "stylelint --cache -q \"**/*.css\"",
        "lint:prettier": "prettier --check \"**/*.{js,ts,tsx,json,html,css,cjs,mjs}\"",
        "lint": "run-s lint:*",
        "fix:prettier": "prettier --write \"**/*.{js,ts,tsx,json,html,css,cjs,mjs}\""
    },
    "dependencies": {
        "@kobalte/core": "^0.13.7",
        "@solidjs/router": "^0.15.2",
        "clsx": "^2.1.1",
        "solid-js": "^1.9.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.17.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-solid": "^0.14.5",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "solid-devtools": "^0.31.1",
        "stylelint": "^16.11.0",
        "stylelint-config-standard": "^36.0.1",
        "typescript": "~5.6.2",
        "typescript-eslint": "^8.18.0",
        "vite": "^6.0.1",
        "vite-plugin-eslint2": "^5.0.3",
        "vite-plugin-solid": "^2.11.0"
    }
}
